            <TableColumn prefWidth="75.0" text="Order Time" />
            <TableColumn prefWidth="75.0" text="Employee Id" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <BorderPane.margin>
            <Insets bottom="36.0" left="36.0" right="36.0" />
         </BorderPane.margin>
      </TableView>
   </center>
   <top>
       <Pane prefHeight="109.0" prefWidth="600.0" BorderPane.alignment="CENTER">
           <Label layoutX="36.0" layoutY="24.0" text="HISTORY" textFill="WHITE">
              <font>
